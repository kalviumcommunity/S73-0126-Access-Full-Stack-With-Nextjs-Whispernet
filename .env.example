# .env.example (Template)

# --- Server-Side Secrets (NEVER exposed to browser) ---
# Connection string for PostgreSQL
DATABASE_URL=
# Private key for backend services (or your DB password)
DB_PASSWORD=

# --- Client-Side Variables (Safe to expose) ---
# Must start with 'NEXT_PUBLIC_'
NEXT_PUBLIC_ENV_NAME=
NEXT_PUBLIC_API_URL=
NEXT_PUBLIC_APP_NAME="Rural School Portal"